  -p distribution uniform
wdir: ..
deps:
- path: draw_samples.ipynb
outs:
- path: draw_samples_output.ipynb
  cache: true
  metric: false
  persist: false
- path: samples.npy
  cache: true
  metric: false
  persist: false


